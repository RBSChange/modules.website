<?xml version="1.0" encoding="UTF-8"?>
<panel>
	<section labeli18n="modules.website.document.page.TitleInformations">
		<field name="navigationtitle"/>
		<field name="template"/>
	</section>
	<xul>
		<javascript>						
			<method name="preparePropertiesValues">
				<parameter name="values" />
