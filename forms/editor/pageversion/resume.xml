<?xml version="1.0"?>
<panel>
	<section name="properties" linkedtab="properties"
		labeli18n="modules.uixul.bo.doceditor.section.Properties" image="{IconsBase}/small/edit-properties.png">
		<property name="label" labeli18n="modules.uixul.bo.doceditor.property.Label" />
		<property name="author" labeli18n="modules.uixul.bo.doceditor.property.Author" />
		<property name="creationdate"
			labeli18n="modules.uixul.bo.doceditor.property.Creationdate" />
	</section>

	<section name="content" labeli18n="modules.uixul.bo.doceditor.section.Content"
		image="{IconsBase}/small/page-content.png">
		<property name="pagecomposition" class="action" type="editPageContent"
			labeli18n="modules.website.bo.doceditor.property.Page-composition" />
		<property name="freecontent"
			labeli18n="modules.website.bo.doceditor.property.Free-content" />
	</section>

	<section name="localization" linkedtab="localization"
		labeli18n="modules.uixul.bo.doceditor.section.Localization" image="{IconsBase}/small/translate.png">
		<property name="languages" type="langs"
			labeli18n="modules.uixul.bo.doceditor.property.Languages" />
	</section>

	<section name="publication" linkedtab="publication"
		labeli18n="modules.uixul.bo.doceditor.section.Publication" image="{IconsBase}/small/status.png">
		<property name="status" labeli18n="modules.uixul.bo.doceditor.property.Status" />
		<property name="workflow" labeli18n="modules.uixul.bo.doceditor.property.Workflow" />
	</section>

	<section flex="1" name="urlrewriting"
		labeli18n="modules.uixul.bo.doceditor.section.Urlrewriting" image="{IconsBase}/small/urlrewriting.png">
		<property name="currenturl"
			labeli18n="modules.uixul.bo.doceditor.property.Currenturl" />
	</section>

	<section name="history" linkedtab="history"
		actiontexti18n="modules.uixul.bo.doceditor.section.action.View"
		labeli18n="modules.uixul.bo.doceditor.section.History" image="{IconsBase}/small/history.png">
		<property name="modificationdate"
			labeli18n="modules.uixul.bo.doceditor.property.Modificationdate" />
	</section>
</panel>